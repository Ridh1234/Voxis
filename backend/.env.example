# Environment Variables for Voice Over AI Agent

# Server Configuration
PORT=3001
NODE_ENV=development

# ElevenLabs API Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_BASE_URL=https://api.elevenlabs.io/v1

# Exotel API Configuration
EXOTEL_API_KEY=your_exotel_api_key_here
EXOTEL_API_TOKEN=your_exotel_api_token_here
EXOTEL_SID=your_exotel_sid_here
EXOTEL_BASE_URL=https://api.exotel.com/v1/Accounts

# Virtual Number Configuration
EXOTEL_VIRTUAL_NUMBER=your_virtual_number_here

# Audio Storage
AUDIO_STORAGE_PATH=./audio

# CORS Configuration
FRONTEND_URL=http://localhost:3000

# API Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
